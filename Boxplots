#Boxplot showing Categorical Data per Tributary 
boxplot(DissectionsK$Fulton.s.K ~ DissectionsK$Number_Of_Parasite_Species)
ggplot(DissectionsK) +
  aes(x = Number_Of_Parasite_Species, y = Fulton.s.K, colour = Fulton.s.K) +
  geom_boxplot()

p <- ggplot(DissectionsK, aes(x=Number_Of_Parasite_Species, y=Fulton.s.K)) + 
  geom_boxplot()
p

p<-ggplot(DissectionsK, aes(x=Number_Of_Parasite_Species, y=Fulton.s.K, color=Number_Of_Parasite_Species)) +
  geom_boxplot()


#Boxplot showing K and number of parasite species 
boxplot(DissectionsK$Fulton.s.K ~ DissectionsK$Number_Of_Parasite_Species)
ggplot(DissectionsK) +
  aes(group= x = Number_Of_Parasite_Species, y = Fulton.s.K, colour = Fulton.s.K) +
  geom_boxplot()


# Multiple boxplot using dataset
boxplot( Fulton.s.K~Number_Of_Parasite_Species,
         data=Key,
         main="Parasite Richness compared to the Condition Factor of Striped Bass",
         xlab="Number of Parasite Taxa",
         ylab="Fulton's Condition Factor",
         col = "Light blue",
         border="black")
  geom_boxplot()
